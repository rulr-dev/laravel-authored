{
    "name": "rulr/laravel-authored",
    "description": "A Laravel package that adds created_by and last_updated_by attributes to Eloquent models.",
    "type": "library",
    "keywords": ["laravel", "eloquent", "authorship", "tracking", "created_by", "updated_by"],
    "license": "MIT",
    "authors": [
        {
            "name": "Raffi",
            "email": "raffi@rulr.dev",
            "homepage": "https://rulr.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "Rulr\\Authored\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rulr\\Authored\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rulr\\Authored\\AuthoredServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
